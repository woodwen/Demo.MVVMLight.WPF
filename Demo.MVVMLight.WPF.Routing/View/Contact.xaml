<Page x:Class="Demo.MVVMLight.WPF.Routing.View.Contact"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Demo.MVVMLight.WPF.Routing.View"
             DataContext="{Binding Contact,Source={StaticResource Locator}}"  
             mc:Ignorable="d" 
             d:DesignHeight="305" d:DesignWidth="669">
    <Grid>
        <Label x:Name="lViewTitle" Content="{Binding Path=ViewTitle,UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="3,12,0,0" VerticalAlignment="Top"/>

    </Grid>
</Page>
